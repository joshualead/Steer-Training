<%-include('header')%>
<body>
  <div class="card m-4">
    <div class="card-body">
      <h3 class="float-start">Scores for "<%=taskid%>-<%=taskname%>"</h3>
      <button class="btn btn-success float-end" onclick="exceller()">Download</button>
    </div>
    <input class="form-control m-3 w-50" type="search" placeholder="Search Trainee Name" aria-label="Search" id="myInput" onkeyup="myFunction()">
  </div>
  <div class="card m-4">
    <div class="card-body m-4">
      <table class="table" id="myTable">
        <thead>
          <tr>
            <th>S.no</th>
            <th>Trainee's ID</th>
            <th>Trainee's Name</th>
            <th>Score</th>
          </tr>
        </thead>
        <tbody>
          <%for(var i=0;i<scoreArr.length;i++){%>
          <tr>
            <td><%=i+1%></td>
            <td><%=scoreArr[i].trainee_id%></td>
            <td><%=scoreArr[i].trainee_name%></td>
            <td><%=scoreArr[i].score%></td>            
            <td><a class="btn btn-outline-success" href="/trainer/view/submission/<%=token%>/<%=taskname%>/<%=scoreArr[i].trainee_name%>/<%=scoreArr[i].score_id%>">View</a></td>
          </tr>
          <%}%>
        </tbody>
      </table>
    </div>
  </div>
</body>
<%-include('footer')%>